<div class='alert alert-danger'>Are you sure you want to delete this dataset?</div>

<div class='jumbotron'>
<h3><%= @dataset.name %></h3>
<p class='lead'>
<%= link_to @dataset, method: :delete, confirm: "Are you sure?", class: 'btn btn-default' do %>
    <i class='fa fa-trash-o'></i> Delete <i class='fa fa-check-square'></i>
<% end %>
</p>
</div>
