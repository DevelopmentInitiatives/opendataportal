<script type="text/javascript">
    $(document).ready(function(){
        var seriesData = [];
        for (var i=0; i < gon.data_values.length; i++){
            var obj = gon.data_values[i];
            seriesData.push(obj);
        }

        $(function () {
                $('#container').highcharts({
                    chart: {
                        type: 'bar'
                    },
                    title: {
                        text: gon.chartTitle
                    },
                    xAxis: {
                        categories: gon.headings_gon
                    },
                    yAxis: {
                        min: 0,
                        title: {
                            text: gon.yLabel
                        }
                    },
                    legend: {
                        backgroundColor: '#FFFFFF',
                        reversed: true
                    },
                    plotOptions: {
                        series: {
                            stacking: 'normal'
                        }
                    },
                        series: seriesData
                });
            });
});
</script>
