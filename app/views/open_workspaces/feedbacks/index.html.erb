<h3>Feedback</h3>
<div class='row'>
  <table>
    <thead>
      <tr>
        <th>
          First Name
        </th>
        <th>
          Last Name
        </th>
        <th>
          Gender
        </th>
        <th>
          Remark
        </th>
      </tr>
    </thead>
    <tbody>
      <% @feedbacks.each do |feedback| %>
      <tr>
        <td>
          <%= feedback.first_name %>
        </td>
        <td>
          <%= feedback.last_name %>
        </td>
        <td>
          <%= feedback.gender.to_s %>
        </td>
        <td>
          <%= feedback.remarks %>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
